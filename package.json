{
  "name": "@decentralchain/data-entities",
  "description": "Domain model classes for DecentralChain blockchain entities",
  "version": "2.0.7",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Decentral-America/data-entities.git"
  },
  "homepage": "https://github.com/Decentral-America/data-entities",
  "bugs": {
    "url": "https://github.com/Decentral-America/data-entities/issues"
  },
  "keywords": [
    "decentralchain",
    "blockchain",
    "data-entities",
    "asset",
    "money",
    "order-price"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "tsc --declaration && npm run build && npm run uglify",
    "uglify": "node_modules/.bin/uglifyjs ./dist/data-entities.js -o ./dist/data-entities.min.js",
    "build": "./node_modules/.bin/browserify ./dist/index.js --node -s data-entities -u @decentralchain/bignumber -o ./dist/data-entities.js",
    "prepublish": "npm test",
    "postversion": "npm publish && git push",
    "test": "./node_modules/.bin/tsc -p ./test/tsconfig.json && ./node_modules/.bin/mocha $(find ./tmp -name '*.spec.js')"
  },
  "dependencies": {
    "@decentralchain/bignumber": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "typescript": "^3.0.3",
    "uglify-js": "^3.4.9"
  }
}
